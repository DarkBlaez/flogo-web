{
  "name": "diagram",
  "version": "0.1.0",
  "description": "FloGo diagram POC",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "postinstall": "typings install",
    "clean:tsc": "find app -name *.js -delete -o -name *.js.map -delete",
    "clean": "find app -name *.d.ts -delete; npm run clean:tsc"
  },
  "keywords": [
    "flogo",
    "diagram",
    "angular2",
    "d3"
  ],
  "author": "G JING <gjing@tibco-support.com>",
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "bluebird": "^3.3.4",
    "bootstrap": "^3.3.6",
    "d3": "^3.5.16",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.1",
    "lodash": "^4.6.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "systemjs": "^0.19.24",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.7",
    "typings": "^0.7.9"
  }
}
