  "version": "0.2.0",
  "configurations": [
    {
      "type": "node2",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/dist/server/server.js",

      "cwd": "${workspaceRoot}/dist/server",
       "diagnosticLogging": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/dist/server/**/*.map.js"
      ],
      "stopOnEntry": true,
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*.js",
        "${workspaceRoot}/src/server/node_modules/**/*.js",
        "${workspaceRoot}/dist/server/node_modules/**/*.js"
      ],
      "runtimeExecutable": "/Users/fcastill/.nvm/versions/node/v6.4.0/bin/node"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "outFiles": []
    }
  ]
}
