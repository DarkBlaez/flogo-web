  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean:all-node-modules": "del **/node_modules",
    "clean:engines": "del ./dist/local/engines",
    "clean:local": "del ./dist/local",
    "update-global-flogo": "go get -u github.com/TIBCOSoftware/flogo-cli/...",
    "submodules:link": "yarn --cwd src/parser link && yarn link flogo-parser && yarn --cwd src/client link flogo-parser && yarn --cwd src/server link flogo-parser",
    "services:status": "docker-compose -p flogo-dev -f docker/services.docker-compose.yml ps",
    "services:pull": "docker-compose -p flogo-dev -f docker/services.docker-compose.yml pull",
    "services:start": "docker-compose -p flogo-dev -f docker/services.docker-compose.yml up -d",
    "services:stop": "docker-compose -p flogo-dev -f docker/services.docker-compose.yml stop flogo-flow-service flogo-state-service",
    "services:clear": "docker-compose -p flogo-dev -f docker/services.docker-compose.yml down",
    "release": "npm-run-all -l release:build-palette submodules:link release:client-version release:pre-build-parser release:server release:bundle-client release:parser release:build-engines release:copy-docker-files",
    "release:server": "node -r esm tools/release/build-server.js",
    "release:pre-build-parser": "yarn --cwd src/parser run installbuild",
    "release:parser": "node -r esm tools/release/build-parser.js",
    "release:build-palette": "node -r esm tools/release/build-palette.js",
    "release:client-version": "node -r esm tools/release/client-version.js",
    "release:bundle-client": "node -r esm tools/release/bundle-client",
    "release:build-engines": "node -r esm tools/release/build-engines.js",
    "release:copy-docker-files": "node -r esm tools/release/copy-docker-files.js"
  },
  "author": "TIBCO Software Inc.",
