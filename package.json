{
  "name": "flogo-web",
  "version": "0.5.4",
  "description": "Wonderful web application for Flogo",
  "license": "SEE LICENSE IN TIBCO Samples Internal End User License.txt",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "scripts": {
    "start": "gulp",
    "postinstall": "yarn run submodules:link",
    "gulp": "gulp",
    "dev": "concurrently --kill-others --names \"PARSER,DEV:SERVER\"  -c \"bgBlue,bgCyan,bgMagenta\" \"yarn --cwd src/parser run build:watch\" \"yarn run dev:server\"",
    "dev:server": "gulp dev-server",
    "watch:server-parser": "concurrently --kill-others --names \"PARSER,SERVER\"  -c \"bgBlue,bgCyan,bgMagenta\" \"yarn run watch:parser\" \"yarn run watch:server\"",
    "watch:server": "gulp dev-watch",
    "watch:parser": "yarn --cwd src/parser run build:watch",
    "test": "yarn test:parser && yarn test:server && yarn test:client",
    "test:client": "yarn --cwd src/client run test:once",
    "test:server": "yarn --cwd src/server run test",
    "test:parser": "yarn --cwd src/parser run test",
    "install:client": "yarn --cwd src/client install",
    "install:server": "yarn --cwd src/server install",
    "install:parser": "yarn --cwd src/parser install",
    "webdriver-manager": "webdriver-manager update && webdriver-manager start",
    "webdriver-manager-update": "webdriver-manager update",
    "e2e": "cd specs && protractor protractor.conf.js",
    "e2e-container": "cd specs && docker run -it --rm --net=host -e FLOGO_HOST=\"$FLOGO_HOST\" -e FLOGO_PORT=\"$FLOGO_PORT\" --name flogo-protractor-runner -v $(pwd):/protractor/project hortonworks/docker-e2e-protractor protractor.conf.js",
    "clean:all-node-modules": "rm -rfv `find . -path \"*/node_modules\" -print`",
    "clean:engines": "rm -rf ./dist/server/local/engines",
    "clean:local": "rm -rf -rf ./dist/server/local",
    "update-global-flogo": "go get -u github.com/TIBCOSoftware/flogo-cli/...",
    "submodules:link": "yarn --cwd src/parser link && yarn link flogo-parser && yarn --cwd src/client link flogo-parser && yarn --cwd src/server link flogo-parser"
  },
  "author": "TIBCO Software Inc.",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.6.1",
    "command-exists": "^1.2.2",
    "concurrently": "^3.5.1",
    "del": "3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "7.0.0",
    "gulp-changed": "^3.1.1",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "5.0.1",
    "gulp-help": "^1.6.1",
    "gulp-nodemon": "^2.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
    "protractor": "5.2.0",
    "require-dir": "^0.3.0",
    "run-sequence": "2.2.0",
    "vinyl-fs": "3.0.0",
    "webdriver-manager": "12.0.6"
  },
  "dependencies": {}
}
