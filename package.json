{
  "name": "flogo-web",
  "version": "0.5.2",
  "description": "Wonderful web application for Flogo",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "scripts": {
    "start": "gulp",
    "gulp": "gulp",
    "dev:server": "gulp dev-server",
    "test:client": "npm --prefix=\"src/client\" run test",
    "test:server": "npm --prefix=\"dist/server\" run test:api -- ./tests",
    "webdriver-manager": "webdriver-manager update && webdriver-manager start",
    "webdriver-manager-update": "webdriver-manager update",
    "e2e": "cd specs && protractor protractor.conf.js",
    "e2e-container": "cd specs && docker run -it --rm --net=host -e FLOGO_HOST=\"$FLOGO_HOST\" -e FLOGO_PORT=\"$FLOGO_PORT\" --name flogo-protractor-runner -v $(pwd):/protractor/project hortonworks/docker-e2e-protractor protractor.conf.js",
    "clean:all-node-modules": "rm -rfv `find . -path \"*/node_modules\" -print`",
    "clean:engines": "rm -rf ./dist/server/local/engines",
    "clean:local": "rm -rf -rf ./dist/server/local",
    "update-global-flogo": "go get -u github.com/TIBCOSoftware/flogo-cli/..."
  },
  "author": "TIBCO Software Inc.",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.6.1",
    "command-exists": "^1.2.2",
    "del": "3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "7.0.0",
    "gulp-changed": "^3.1.1",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "5.0.1",
    "gulp-help": "^1.6.1",
    "gulp-nodemon": "^2.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
    "protractor": "5.2.0",
    "require-dir": "^0.3.0",
    "run-sequence": "2.2.0",
    "vinyl-fs": "3.0.0",
    "webdriver-manager": "12.0.6"
  },
  "dependencies": {}
}
