{
  "name": "flogo-parser",
  "version": "0.5.5",
  "description": "Flogo mapping parser",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "author": "TIBCO Software",
  "license": "SEE LICENSE IN TIBCO Samples Internal End User License.txt",
  "bugs": {
    "url": "https://github.com/TIBCOSoftware/flogo-web/issues"
  },
  "homepage": "https://github.com/TIBCOSoftware/flogo-web#readme",
  "engines": {
    "node": ">=8.9.1 <9.0.0",
    "yarn": ">=1.3.2 <2.0.0"
  },
  "scripts": {
    "lint": "tslint -c tslint.json './lib/**/*.ts'",
    "build": "tsc -p tsconfig.dist.json",
    "build:watch": "tsc -p tsconfig.dist.json --watch",
    "test": "mocha --require ts-node/register './{,!(node_modules)/**/}*.spec.ts'",
    "test:watch": "mocha --watch --require ts-node/register --watch-extensions json './{,!(node_modules)/**/}*.spec.ts'",
    "installbuild": "yarn install && yarn run build"
  },
  "dependencies": {
    "chevrotain": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
