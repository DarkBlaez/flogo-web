{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "es2015",
    "baseUrl": "",
    "types": [],
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "./test.ts",
    "./**/*.spec.ts",
    "./**/*.mock.ts"
  ]
}
