{
  "name": "flogo-web-server",
  "version": "0.5.4",
  "description": "Wonderful web application for Flogo",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start-server": "node server.js",
    "start-debug": "node --debug server.js",
    "start-prod-server": "node server.js",
    "test": "mocha --require babel-core/register './{,!(node_modules)/**/}*.spec.js' --preserve-symlinks",
    "test:watch": "mocha --watch --require babel-core/register --watch-extensions json './{,!(node_modules)/**/}*.spec.js' --preserve-symlinks",
    "test:api": "mocha"
  },
  "author": "TIBCO Software Inc.",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.6.1",
    "chai": "4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.1",
    "mocha": "4.0.1",
    "server-destroy": "^1.0.1",
    "sinon": "^4.4.1"
  },
  "peerDependencies": {
    "flogo-parser": "",
    "chevrotain": "^1.0.1"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "atob": "^2.0.3",
    "co-request": "1.0.0",
    "cross-spawn": "^5.1.0",
    "fs-extra": "4.0.2",
    "hosted-git-info": "2.5.0",
    "koa": "1.4.1",
    "koa-body": "1.6.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-json": "2.0.2",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.6.1",
    "nedb": "1.8.0",
    "npm": "5.5.1",
    "performance-now": "2.1.0",
    "ps-node": "^0.1.4",
    "semver": "5.4.1",
    "shortid": "^2.2.8",
    "socket.io": "2.0.4",
    "winston": "2.4.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-unused-expressions": "off",
      "func-names": "off",
      "max-len": [
        "error",
        120,
        2,
        {
          "ignoreUrls": true,
          "ignoreComments": false,
          "ignoreRegExpLiterals": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "import/prefer-default-export": [
        "off"
      ],
      "no-console": "off",
      "no-param-reassign": "off",
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "no-debugger": [
        "error"
      ],
      "no-trailing-spaces": [
        "off"
      ],
      "dot-notation": [
        "warn"
      ],
      "no-confusing-arrow": [
        "warn",
        {
          "allowParens": true
        }
      ],
      "prefer-arrow-callback": [
        "warn"
      ],
      "arrow-parens": [
        "warn",
        "as-needed"
      ],
      "class-methods-use-this": [
        "warn"
      ],
      "vars-on-top": [
        "warn"
      ]
    }
  }
}
