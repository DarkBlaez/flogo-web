{
  "name": "flogo-web-server",
  "version": "0.5.6",
  "description": "Wonderful web application for Flogo",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "nodemon",
    "start-server": "babel-node --extensions '.js,.ts' server.js",
    "start-debug": "babel-node server.js",
    "start-prod-server": "babel-node server.js",
    "test": "mocha --require setup-tests.js --preserve-symlinks './{,!(node_modules)/**/}*.spec.{js,ts}'",
    "test:watch": "mocha --watch ---require setup-tests.js --watch-extensions json --preserve-symlinks './{,!(node_modules)/**/}*.spec.{js,ts}'",
    "test:api": "mocha",
    "serve": "babel-node --extensions '.js,.ts' server.js",
    "build": "node scripts/build",
    "configure-engines": "babel-node --extensions '.js,.ts' configure-engines.js",
    "build:transpile": "babel --extensions '.js,.ts' --ignore '**/tests/**,**/node_modules/**,*.spec.{js,ts}' ."
  },
  "author": "TIBCO Software Inc.",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "8.9.3",
    "chai": "4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.1",
    "cpy": "^7.0.1",
    "mocha": "4.0.1",
    "nodemon": "^1.18.4",
    "server-destroy": "^1.0.1",
    "sinon": "^4.4.1",
    "typescript": "^3.0.3"
  },
  "peerDependencies": {
    "chevrotain": "^3.0.0",
    "flogo-parser": ""
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "atob": "^2.0.3",
    "co-request": "1.0.0",
    "cross-spawn": "^5.1.0",
    "dotenv": "^6.0.0",
    "find-up": "^3.0.0",
    "fs-extra": "4.0.2",
    "hosted-git-info": "2.5.0",
    "koa": "1.4.1",
    "koa-body": "1.6.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-json": "2.0.2",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.6.1",
    "nedb": "1.8.0",
    "npm": "5.5.1",
    "npm-run-all": "^4.1.3",
    "performance-now": "2.1.0",
    "ps-node": "^0.1.4",
    "semver": "5.4.1",
    "shortid": "^2.2.8",
    "socket.io": "2.0.4",
    "winston": "2.4.0"
  }
}
