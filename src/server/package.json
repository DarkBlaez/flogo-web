{
  "name": "flogo-web-server",
  "version": "0.1.0",
  "description": "Wonderful web application for Flogo",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "scripts": {
    "start": "concurrently \"pouchdb-server --host 0.0.0.0 -p 5984 -d './docker-shared'\" \"node server.js\"",
    "start-db": "pouchdb-server -p 5984 -d ./docker-shared --host 0.0.0.0",
    "start-server": "npm run start-prod-server",
    "start-dev-server": "babel-node server.js",
    "start-prod-server": "node server.js",
    "dump-db": "mkdir db-init && pouchdb-dump http://0.0.0.0:5984/flogo-web-activities > db-init/installed-activities.init && pouchdb-dump http://0.0.0.0:5984/flogo-web-triggers > db-init/installed-triggers.init   && pouchdb-dump http://0.0.0.0:5984/flogo-web > db-init/installed-flows.init"
  },
  "author": "Shaoke Xu<shaoxu@tibco.com>",
  "devDependencies": {
    "pouchdb-dump-cli": "^2.0.4"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "co-request": "1.0.0",
    "concurrently": "^2.0.0",
    "fs-extra": "^0.30.0",
    "koa": "^1.2.0",
    "koa-body": "",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-json": "",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.6.1",
    "npm": "^3.8.5",
    "performance-now": "^0.2.0",
    "pouchdb": "^5.3.1",
    "pouchdb-load": "^1.4.6",
    "pouchdb-server": "^1.1.1",
    "semver": "^5.2.0"
  }
}
